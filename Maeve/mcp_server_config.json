{
  "mcpServers": {
    "rag": {
      "name": "RAG",
      "transport": "stdio",
      "description": "Query ingested documents.",
      "icon": "document-magnifying-glass",
      "command": "docker-compose",
      "args": [
        "-f",
        "../docker-compose.dev.yaml",
        "exec",
        "rag-mcp-server",
        "uv",
        "run",
        "mcp",
        "run",
        "main.py"
      ]
    },
    "finch": {
      "name": "Finch music player",
      "transport": "http-streaming",
      "description": "Lookup and play albums, playlists and tracks.",
      "icon": "musical-note",
      "endpoint": "http://127.0.0.1:8018/sse"
    },
    "applescript-mcp": {
      "name": "AppleScript",
      "transport": "stdio",
      "description": "Interact with macOS through AppleScript.",
      "icon": "code-bracket-square",
      "command": "node",
      "args": [
        "../mcp-servers/applescript-mcp/dist/index.js"
      ]
    },
    "hue-mcp": {
      "name": "Philips Hue",
      "transport": "http-streaming",
      "description": "Interface for controlling Philips Hue smart lightning systems.",
      "icon": "light-bulb",
      "endpoint": "http://127.0.0.1:8023/sse"
    }
  }
}