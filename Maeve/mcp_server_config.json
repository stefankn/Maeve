{
  "mcpServers": {
    "rag": {
      "name": "RAG",
      "description": "Query ingested documents.",
      "icon": "document-magnifying-glass",
      "command": "docker-compose",
      "args": [
        "-f",
        "../docker-compose.dev.yaml",
        "exec",
        "rag-mcp-server",
        "uv",
        "run",
        "mcp",
        "run",
        "main.py"
      ]
    },
    "finch": {
      "name": "Finch music database",
      "description": "Lookup playable albums, playlists and tracks.",
      "icon": "musical-note",
      "command": "dotnet",
      "args": [
        "run",
        "--project",
        "../mcp-servers/FinchMcpServer/FinchMcpServer.csproj"
      ]
    },
    "hue-mcp": {
      "name": "Philips Hue",
      "description": "Interface for controlling Philips Hue smart lightning systems.",
      "command": "docker-compose",
      "icon": "light-bulb",
      "args": [
        "-f",
        "../docker-compose.dev.yaml",
        "exec",
        "hue-mcp-server",
        "uv",
        "run",
        "mcp",
        "run",
        "hue_server.py"
      ]
    }
  }
}