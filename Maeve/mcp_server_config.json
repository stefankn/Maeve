{
  "mcpServers": {
    "rag": {
      "command": "docker-compose",
      "args": [
        "-f",
        "../docker-compose.dev.yaml",
        "exec",
        "rag-mcp-server",
        "uv",
        "run",
        "mcp",
        "run",
        "main.py"
      ]
    },
    "finch": {
      "command": "dotnet",
      "args": [
        "run",
        "--project",
        "../FinchMcpServer/FinchMcpServer.csproj"
      ]
    },
    "hue-mcp": {
      "command": "docker-compose",
      "args": [
        "-f",
        "../docker-compose.dev.yaml",
        "exec",
        "hue-mcp-server",
        "uv",
        "run",
        "mcp",
        "run",
        "hue_server.py"
      ]
    }
  }
}